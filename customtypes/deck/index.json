{
  "format": "page",
  "id": "deck",
  "label": "Deck",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading1,strong,em"
        }
      },
      "theme": {
        "type": "Select",
        "config": {
          "label": "Theme",
          "placeholder": "Slider theme color",
          "options": ["orange", "white", "pink", "green", "purple", "blue"],
          "default_value": "orange"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "generic_image_slice": {
              "type": "SharedSlice"
            },
            "company": {
              "type": "SharedSlice"
            },
            "pricing": {
              "type": "SharedSlice"
            },
            "offer": {
              "type": "SharedSlice"
            },
            "features": {
              "type": "SharedSlice"
            },
            "testimonials": {
              "type": "SharedSlice"
            },
            "resources": {
              "type": "SharedSlice"
            },
            "references": {
              "type": "SharedSlice"
            },
            "agenda": {
              "type": "SharedSlice"
            },
            "value_proposition": {
              "type": "SharedSlice"
            },
            "product": {
              "type": "SharedSlice"
            },
            "methodology": {
              "type": "SharedSlice"
            },
            "leads_goals": {
              "type": "SharedSlice"
            },
            "intro_slide": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "Config": {
      "company_name": {
        "type": "Text",
        "config": {
          "label": "Company name",
          "placeholder": ""
        }
      },
      "company_logo": {
        "type": "Image",
        "config": {
          "label": "Company Logo",
          "constraint": {},
          "thumbnails": []
        }
      },
      "author": {
        "type": "Link",
        "config": {
          "label": "Author",
          "select": "document",
          "customtypes": ["author"]
        }
      }
    },
    "Access": {
      "activate_password": {
        "type": "Boolean",
        "config": {
          "label": "Activate Password",
          "default_value": false,
          "placeholder_true": "true",
          "placeholder_false": "false"
        }
      },
      "password": {
        "type": "Text",
        "config": {
          "label": "Password",
          "placeholder": ""
        }
      }
    }
  }
}
